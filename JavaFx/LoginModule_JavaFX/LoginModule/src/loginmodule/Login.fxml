<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXPasswordField?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane id="AnchorPane" prefHeight="450.0" prefWidth="550.0" xmlns="http://javafx.com/javafx/8.0.102" xmlns:fx="http://javafx.com/fxml/1" fx:controller="loginmodule.LoginController">
   <children>
      <Pane layoutX="-3.0" prefHeight="450.0" prefWidth="556.0" style="-fx-background-color: #ffffff;">
         <children>
            <JFXTextField fx:id="unametxt" layoutX="171.0" layoutY="169.0" prefHeight="31.0" prefWidth="220.0" promptText="Username" />
            <JFXPasswordField fx:id="pwdtxt" layoutX="171.0" layoutY="213.0" prefHeight="31.0" prefWidth="220.0" promptText="Password" />
            <Label layoutX="221.0" layoutY="276.0" text="New user?" />
            <ImageView fitHeight="67.0" fitWidth="78.0" layoutX="245.0" layoutY="77.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@User.png" />
               </image>
            </ImageView>
            <Label layoutX="300.0" layoutY="252.0" text="Forgot Password?" textFill="#ff2626" />
            <Button fx:id="signupbtn" layoutX="218.0" layoutY="294.0" mnemonicParsing="false" onAction="#signupclick" onMouseEntered="#sme1" prefHeight="31.0" prefWidth="81.0" style="-fx-background-color: #2196f3;" text="Sign up" textFill="WHITE" />
            <Button fx:id="loginbtn" layoutX="312.0" layoutY="294.0" mnemonicParsing="false" onAction="#loginclick" onMouseEntered="#lme2" prefHeight="31.0" prefWidth="81.0" style="-fx-background-color: #2196f3;" text="Login" textFill="#f8f8f8" />
            
         </children>
      </Pane>
   </children>
</AnchorPane>
